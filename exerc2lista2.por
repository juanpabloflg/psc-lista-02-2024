programa {
  funcao inicio() {
    inteiro numeroUm
    inteiro numeroDois
    escreva ("informe os números: ")
    leia (numeroUm)
    leia (numeroDois)
    inteiro soma = (numeroUm + numeroDois)
    escreva ("A soma dos números é: " + soma + "\n") 
    inteiro subtrair = (numeroUm - numeroDois)
    escreva ("A subtração dos números é: " + subtrair + "\n") 
    inteiro multiplicar = (numeroUm * numeroDois)
    escreva ("A multiplicação dos números é: " + multiplicar + "\n")


  }
}
